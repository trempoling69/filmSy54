{% extends 'base.html.twig' %}

{% block title %}TypeArtefact index{% endblock %}

{% block body %}
    <h1>TypeArtefact index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_artefact in type_artefacts %}
            <tr>
                <td>{{ type_artefact.id }}</td>
                <td>{{ type_artefact.nom }}</td>
                <td>
                    <a href="{{ path('app_type_artefact_show', {'id': type_artefact.id}) }}">show</a>
                    <a href="{{ path('app_type_artefact_edit', {'id': type_artefact.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_artefact_new') }}">Create new</a>
{% endblock %}
